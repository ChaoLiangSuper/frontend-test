{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "object-literal-sort-keys": false,
    "arrow-parens": true,
    "no-submodule-imports": false,
    "no-implicit-dependencies": [true, "dev"]
  },
  "rulesDirectory": ["src"]
}
